<section id="page_ruicon">
    <h1>RIDIBOOKS 서점 SVG Icon</h1>
    <p class="icon_name">CSS Background</p>
    <ul class="icon_list">
    {{#each svgList as |svg|}}
      <li class="icon_item">
        <a href="dist/icon/svg_icon/{{svg.name}}.svg" class="icon_link" target="_blank" download>
            <span class="svg_icon svg-{{svg.name}}"></span>
        </a>
        <span class="icon_title">svg_{{svg.name}}<br/>{{svg.viewBox.width}}X{{svg.viewBox.height}}</span>
      </li>
    {{/each}}
    </ul>
</section>
</body>
